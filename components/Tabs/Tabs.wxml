<view class="tabs">
    <view class="tabs_title">
        <view class="title_item {{ item.isActive? 'active': '' }}"
            wx:for="{{tabs}}"
            wx:key="id"
            bindtap = "hadnleItemTap"
            data-index = "{{index}}"
        > 
            <text>{{item.value}}</text>
        </view>
    </view>
    <view class="tabs_content">
        <slot></slot>
    </view>
</view>
